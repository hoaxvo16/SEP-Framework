<Window x:Class="Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="900" FontSize="20">
    <Grid>
        <Grid Name="grid">

        </Grid>
        <StackPanel Name="stackPanel">
            <Button Width="200" Name="Delete" Click="Delete_Click">Delete</Button>
            <Button Width="200" Name="Undo" Click="Undo_Click">Undo</Button>
            <Button Width="200" Name="Redo" Click="Redo_Click">Redo</Button>
            <Button Width="200" Name="AddCol" Click="AddCol_Click">AddCol</Button>
       
        </StackPanel>
    </Grid>
</Window>
